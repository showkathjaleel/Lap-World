    <script>
        function changeQuantity(cartId, productId, userId, count) {
            let qty = document.getElementById(productId).innerHTML
            let Price = document.getElementById(productId+'price').innerHTML


            qty = parseInt(qty)
            count = parseInt(count)
            Price = parseInt(Price)

            $.ajax({
                url: '/change-product-quantity',
                data: {

                    cart: cartId,
                    product: productId,
                    user: userId,
                    count: count,
                    qty: qty
                },
                method: 'post',
                success: (response) => {
                   
                    console.log(response)
                    if (response.response) {
                        document.getElementById(productId).innerHTML = qty + count
                        Price = parseInt(Price)
                        document.getElementById(productId + 'total').innerHTML = document.getElementById(productId).innerHTML * Price
                        document.getElementById('grand').innerHTML=response.grandTotal
                        grandTotal=document.getElementById('grand').innerHTML
                        
                            console.log(grandTotal);  
                    }
                    else {  
                  
                        alert("product removed from cart")
                        location.reload()
                 

                    }
                }
            })
        }

    </script>
